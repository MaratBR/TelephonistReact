{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    "@babel/transform-runtime",
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
